<?xml version='1.0' encoding='utf-8'?>
<widget id="ar.com.dia8publicidad.trinidadpalermo" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Sanatorio de la Trinidad Palermo</name>
    <description>
        Sanatorio de la Trinidad Palermo
    </description>
    <author email="info@dia8publicidad.com.ar" href="dia8publicidad.com.ar">
		Dia8Publicidad
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
		<preference name="android-build-tool" value="gradle" />
		<resource-file src="google-services.json" target="google-services.json" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">	
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
		<resource-file src="GoogleService-Info.plist" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-dialogs" spec="~1.3.3" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="phonegap-plugin-push" spec="1.9.0" />
    <engine name="android" spec="~6.1.2" />
</widget>
