<?xml version='1.0' encoding='utf-8'?>
<widget id="ar.com.iclient" version="1.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="BarcodeScanner">
        <param name="browser-package" value="BarcodeScanner" />
    </feature>
    <name>Iclient</name>
    <description>
        Comprás, ganás
    </description>
    <author email="info@dia8publicidad.com.ar" href="dia8publicidad.com.ar">
		Dia8Publicidad
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="icon.png" />
    <splash src="splash.png" />
    <preference name="DisallowOverscroll" value="true" />
</widget>
